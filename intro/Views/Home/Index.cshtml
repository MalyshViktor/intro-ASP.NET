@{
    ViewData["Title"] = "Home Page";
}

<p>
    @ViewData["rnd"]
</p>

<i>
    @ViewBag.hash
</i>
<br></br>

<i>
   Date - Long format: @ViewData["dt"]
</i>
<br></br>

<i>
    Time - Long Format: @ViewData["tm"]
</i>
<br></br>
<i>
   Date - Short format: @ViewData["dts"]
</i>
<br></br>

<i>
    Time - Short Format: @ViewData["tms"]
</i>
<br></br>
<p>
    Users Count: @ViewData["UsersCount"]
</p>
<br></br>
<p>
    @* Вывод на стартовой странице всех
 * зарегистрированных пользователей*@
    Users Name: 
    @foreach(var names in ViewBag.RealName)
    {
        @names
    }
</p>
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
